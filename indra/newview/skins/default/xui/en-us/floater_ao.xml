<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater
	name="geminifloater"
  rect_control="EmeraldAORect"
	title="Animation Overrider"
	can_resize="false"
	can_minimize="true"
	can_close="true"
	can_drag_on_left="false"
	width="400"
	height="740">

  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-42" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="10"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Notecard:
  </text>
  <button
      name="opencard"
      label="Open"
      font="SansSerifSmall"
      left="70"
      bottom_delta="0"
      width="60"
      height="20"
      follows="bottom|left"
		/>
  <button
      name="reloadcard"
      label="Reload"
      font="SansSerifSmall"
      left="131"
      bottom_delta="0"
      width="60"
      height="20"
      follows="bottom|left"
		/>
  <view_border blevel_style="in" bottom_delta="-20" follows="left|top" height="16" left="10"
       mouse_opaque="false" name="ao_notecard" width="180" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
       bottom_delta="0" drop_shadow_visible="true" follows="left|top"
       font="SansSerifSmall" h_pad="0" halign="center" height="16" left_delta="0"
       mouse_opaque="true" name="Give inventory"
       tool_tip="Drop a ZHAO notecard here. Animations have to be in the same Inventory folder as the notecard." v_pad="2"
       width="180">
    Drop a ZHAO II Notecard here.
  </text>
  <view_border blevel_style="in" bottom_delta="-18" follows="left|top" height="16" left_delta="0"
       mouse_opaque="false" name="ao_notecard_disp" width="180" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
       bottom_delta="0" drop_shadow_visible="true" follows="left|top"
       font="SansSerifSmall" h_pad="0" halign="center" height="16" left_delta="0"
       mouse_opaque="true" name="ao_nc_text"
       tool_tip="" v_pad="2"
       width="180">
    Currently set to: ITEM
  </text>

  <check_box bottom_delta="-42" control_name="EmeraldAOEnabled" enabled="true"
	 follows="left|top" font="SansSerifSmall" height="16" initial_value="false" label="Override Anims" left="10"
	 mouse_opaque="true" name="EmeraldAOEnabled" radio_style="false" width="90" />
  <check_box bottom_delta="0" control_name="EmeraldAOSitsEnabled" enabled="true"
	 follows="left|top" font="SansSerifSmall" height="16" initial_value="true" label="Override Sits" left="110"
	 mouse_opaque="true" name="EmeraldAOSitsEnabled" radio_style="false" width="90" />

  <spinner bottom_delta="-40" decimal_digits="2" follows="left|top" height="16" control_name="EmeraldAOStandInterval"
	 increment="1" initial_val="20" left="10" max_val="9000" min_val="1.00"
	 mouse_opaque="true" name="standtime" label="Stand Time" label_width="80"
	 tool_tip="AO Stand Time in seconds" width="180" />
  <check_box bottom_delta="-20" control_name="EmeraldAONoStandsInMouselook" enabled="true"
	 follows="left|top" font="SansSerifSmall" height="16" initial_value="true" label="Disable Stands in Mouselook" left="10"
	 mouse_opaque="true" name="EmeraldAONoStandsInMouselook" radio_style="false" width="180" />

  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="140" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Stands:
  </text>
  <combo_box name="stands" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20"/>
  <button
    name="prevstand"
    tool_tip="previous stand"
    label="&lt;&lt;"
    font="SansSerifSmall"
    left="210"
    bottom_delta="-22"
    width="50"
    height="20"
    follows="bottom|left"
		/>
  <button
  name="nextstand"
  tool_tip="next stand"
  label="&gt;&gt;"
  font="SansSerifSmall"
  left="260"
  bottom_delta="0"
  width="50"
  height="20"
  follows="bottom|left"
		/>
  <check_box bottom_delta="0" control_name="EmeraldAOStandRandomize" enabled="true"
 follows="left|top" font="SansSerifSmall" height="16" initial_value="false" label="randomize" left="310"
 mouse_opaque="true" name="EmeraldAOStandRandomize" radio_style="false" width="90" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Walk:
  </text>
  <combo_box name="walks" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultWalk" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Run:
  </text>
  <combo_box name="runs" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultRun" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Jump:
  </text>
  <combo_box name="jumps" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultJump" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Sit:
  </text>
  <combo_box name="sits" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultSit" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Groundsit:
  </text>
  <combo_box name="gsits" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultGroundSit" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Crouch:
  </text>
  <combo_box name="crouchs" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultCrouch" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Crouchwalk:
  </text>
  <combo_box name="cwalks" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultCrouchWalk" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Fall:
  </text>
  <combo_box name="falls" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultFall" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Hover:
  </text>
  <combo_box name="hovers" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultHover" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Fly:
  </text>
  <combo_box name="flys" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultFly" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Slow Fly:
  </text>
  <combo_box name="flyslows" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultFlySlow" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
