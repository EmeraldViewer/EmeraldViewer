  <combo_box name="standups" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultStandUp" />
  <text bg_visible="false" border_drop_shadow_visible="false" border_visible="false"
	     bottom_delta="-20" drop_shadow_visible="true" follows="left|bottom"
	     font="SansSerifSmall" h_pad="0" halign="left" height="16" left="210"
	     mouse_opaque="true" name="buttons_desc" v_pad="0" width="180">
    Default Pre-Jump:
  </text>
  <combo_box name="prejumps" label="" follows="left|top" height="20" left="210" width="180" bottom_delta="-20" control_name="EmeraldAODefaultPreJump" />
</floater>